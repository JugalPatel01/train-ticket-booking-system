<h1>Places</h1>

<div id="places">
  <% @places.each do |place| %>
    <%= render place %>
    <p>
      <%= link_to "Show this place", place %>
    </p>
  <% end %>
</div>

<%= link_to "New place", new_place_path %>
<%= link_to 'Back to Schedules', schedules_path, class:"btn btn-primary mb-2 mt-2" %>

